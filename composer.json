{
    "name": "oscarbarrett/craft-asset-lockdown",
    "description": "Extends Craft CMS permissions to allow locking down access to the Assets Page but still allow uploading.",
    "type": "craft-plugin",
    "version": "1.0.1",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "asset lockdown"
    ],
    "support": {
        "docs": "https://github.com/OscarBarrett/craft-asset-lockdown/blob/master/README.md",
        "issues": "https://github.com/OscarBarrett/craft-asset-lockdown/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Oscar Barrett",
            "homepage": "https://github.com/OscarBarrett/craft-asset-lockdown"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.0"
    },
    "autoload": {
        "psr-4": {
          "oscarbarrett\\assetlockdown\\": "src/"
        }
    },
    "extra": {
        "name": "Asset Lockdown",
        "handle": "asset-lockdown",
        "hasCpSettings": false,
        "hasCpSection": false,
        "changelogUrl": "https://raw.githubusercontent.com/OscarBarrett/craft-asset-lockdown/master/CHANGELOG.md",
        "class": "oscarbarrett\\assetlockdown\\AssetLockdown"
    }
}
